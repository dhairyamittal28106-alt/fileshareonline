{
    "framework": "nextjs",
    "buildCommand": "npm run build",
    "installCommand": "npm install",
    "crons": [
        {
            "path": "/api/cron/cleanup",
            "schedule": "0 0 * * *"
        }
    ],
    "functions": {
        "app/api/**/*": {
            "maxDuration": 60
        }
    }
}